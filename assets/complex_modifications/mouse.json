{
    "title": "Mac OS Mouse Browser",
    "rules": [{
        "description": "Use button 4 and 5 on Mac OS for browser",
        "manipulators": [{
                "type": "basic",
                "from": {
                    "pointing_button": "button4"
                },
                "to": [{
                    "key_code": "left_arrow",
                    "modifiers": [
                        "left_command"
                    ]
                }]
            },
            {
                "type": "basic",
                "from": {
                    "pointing_button": "button5"
                },
                "to": [{
                    "key_code": "right_arrow",
                    "modifiers": [
                        "left_command"
                    ]
                }]
            }
        ]
    }]
}
